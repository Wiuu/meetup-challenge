{
    "name": "samj/fractal-bundle",
    "license": "MIT",
    "type": "library",
    "description": "A Symfony Bundle for Fractal by League. Implements dependency injection into your transformers",
    "autoload": {
        "psr-4": {
            "SamJ\\FractalBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6",
        "league/fractal": "~0.17",
        "symfony/framework-bundle": "^2.8|^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    }
}
